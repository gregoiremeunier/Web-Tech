<article>
    <header>
        <h1 class="art_title">{{title}}</h1>
    </header>

    <p><b>Content:</b> {{{content}}}</p>
    <p><b>Author:</b> {{author}}</p>
    <p><b>Created on:</b> {{dateCreated}}</p>
    <p><b>Last updated on:</b> {{lastUpdated}}</p>
    <img alt="Image" src={{imageLink}}>
    <p> {{tags}} </p>
</article>
